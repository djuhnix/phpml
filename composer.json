{
    "name": "djuhnix/phpml-pre",
    "description": "A PHP binding of csfml for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Germain Jr. OLEA-OYOUGOU",
            "email": "djuhngleen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "myclabs/php-enum": "^1.7",
        "ext-zend-opcache": "*"
    },
    "autoload": {
        "psr-4": {
            "PHPML\\": "src",
            "PHPML\\AbstractFFI\\": "src/AbstractFFI",
            "PHPML\\Exception\\": "src/Exception",
            "PHPML\\Enum\\": "src/Enum",
            "PHPML\\Event\\": "src/Event",
            "PHPML\\Graphics\\": "src/Graphics",
            "PHPML\\Library\\": "src/Library"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPML\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
